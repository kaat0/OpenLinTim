<?xml version="1.0"?>
<project name="LinTim-sl-helper" default="build-sl-helper" basedir=".">
	<property name="enable_debug" value="true"/>
	<property name="enable_optimize" value="true"/>
	<target depends="build-sl-helper" name="build-all"/>
	<target name="build-sl-helper">
		<javac srcdir="."
		       destdir="."
		       debug="${enable_debug}"
		       optimize="${enable_optimize}"
		       includeAntRuntime="true">
		<compilerarg value="-Xlint"/>
		</javac>
	</target>
</project>
